      "template": {
        "encapsulation": 0,
        "template": "<div class=\"row\" *ngIf=\"tables.length > 0\">\r\n    <div class=\"col l3 m3 s12\">\r\n        <div class=\"collection\">\r\n            <a href=\"#!\" *ngFor=\"let table of tables\"\r\n               [class.active]=\"table == selectedTable\"\r\n               (click)=\"onSelect(table)\"\r\n               class=\"collection-item truncate\">\r\n                {{table.name}}\r\n            </a>\r\n        </div>\r\n    </div>\r\n    <div class=\"col l9 m9 s12\">\r\n        <div *ngIf=\"selectedTable != null\" class=\"card-panel\">\r\n            <h4>{{selectedTable.name}}</h4>\r\n            <table class=\"bordered responsive-table\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>Field</th>\r\n                        <th>Type</th>\r\n                        <th>Arg</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr *ngFor=\"let field of fields\">\r\n                        <td>{{field.name}}</td>\r\n                        <td>{{field.type}}</td>\r\n                        <td>{{field.arg}}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
        "templateUrl": "asset:mocker/lib/components/tables/tables_component.html",
